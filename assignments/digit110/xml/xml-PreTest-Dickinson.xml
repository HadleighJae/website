        <front>
            <div type="references">
                <listRef>
                    <ptr target="xml/1606variant.xml"/>
                    <ptr target="img/1606/fs1606a.jpg"/>
                    <ptr target="img/1606/fs1606b.jpg"/> <!-- 2023-09-01: hjb: There is no = between the attribute and its value -->
